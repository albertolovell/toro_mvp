{
  "name": "toro",
  "version": "1.0.0",
  "author": "Alberto Lovell",
  "description": "a simple stock tracker showing top market movers and a customizable watchlist",
  "main": "./server/server.js",
  "scripts": {
    "react-dev": "webpack --mode development --watch",
    "server-dev": "npx nodemon --watch ./server/server.js",
    "dev": "concurrently \"npm run react-dev\" \"npm run server-dev\"",
    "start": "node ./server/server.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jest": "^29.7.0",
    "mongoose": "^8.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "path": "^0.12.7",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "concurrently": "^8.0.1"
  }
}
